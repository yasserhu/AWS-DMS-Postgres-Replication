        "dataDiskResources": {
            "value": [
                {
                    "name": "postgresvm_DataDisk_0",
                    "sku": "Premium_LRS",
                    "properties": {
                        "diskSizeGB": 1024,
                        "creationData": {
                            "createOption": "empty"
                        }
                    }
                }
            ]
        },
        "virtualMachineSize": {
            "value": "Standard_D2s_v3"
        },
        "adminUsername": {
            "value": "yasserhu"
        },
        "adminPublicKey": {
            "value": null
        },
        "autoShutdownStatus": {
            "value": "Enabled"
        },
        "autoShutdownTime": {
            "value": "19:00"
